   "source": [
    "def f(n):\n",
    "    sum = 0                   # O(1) \n",
    "    for i in range(1, n + 1): # O(n)\n",
    "        sum = sum + i         # O(1)\n",
    "    return sum                # O(1)\n",
    "\n",
    "# T(n) = O(n)\n",
    "# Result is ssum of first n natural numbers,\n",
    "\n",
    "def improved_f(n):\n",
    "    return n * (n+1) // 2 # O(1)"
   ]
